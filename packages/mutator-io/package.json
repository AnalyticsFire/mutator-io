{
  "name": "mutator-io",
  "version": "0.2.4",
  "description": "A tiny library to handle (big) data transformations",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist; tsc -d",
    "build:watch": "rm -rf dist; tsc -w -d",
    "doc": "rm -rf doc && typedoc --options typedoc.js --out ./doc ./src",
    "test": "mocha --recursive --compilers ts:espower-typescript/guess test/helper.ts test",
    "test:watch": "mocha --watch --watch-extensions ts --recursive --compilers ts:espower-typescript/guess test/helper.ts test",
    "test:debug": "iron-node ./node_modules/mocha/bin/_mocha --recursive --compilers ts:espower-typescript/guess helper.ts test.ts",
    "posttest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnalyticsFire/mutator-io.git"
  },
  "author": "Analyticsfire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnalyticsFire/mutator-io/issues"
  },
  "homepage": "https://github.com/AnalyticsFire/mutator-io#readme",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.14",
    "espower-typescript": "^8.0.2",
    "iron-node": "^3.0.19",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.4",
    "proxyquire": "^1.8.0",
    "rxjs": "5.5.2",
    "sinon": "^2.3.8",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.11",
    "typescript": "^2.5.3"
  },
  "peerDependencies": {
    "rxjs": "5.5.2"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "keywords": [
    "typescript",
    "rxjs",
    "transform",
    "data",
    "big",
    "streams"
  ]
}
